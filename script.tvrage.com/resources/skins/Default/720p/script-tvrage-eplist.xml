<window>
  <defaultcontrol>120</defaultcontrol>
  <coordinates>
    <system>1</system>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <animation effect="fade" time="250">WindowOpen</animation>
  <animation effect="fade" time="250">WindowClose</animation>
  <controls>
    <control type="group">
      <posx>200</posx>
      <posy>60</posy>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>890</width>
        <height>580</height>
        <texture border="40">default-panel.png</texture>
      </control>
      <!-- ** Required ** Do not change <id> or <type> -->

        <control type="image" id="102">
            <description>page header tab icon</description>
            <posx>20</posx>
            <posy>12</posy>
            <width>200</width>
            <height>82</height>
            <aspectratio>keep</aspectratio>            
        </control>
             


      <control type="label" id="100">
        <description>textarea</description>
        <posx>200</posx>
        <posy>60</posy>
        <width>400</width>
        <height>40</height>
        <font>font18</font>
        <textcolor>white</textcolor>
        <aligny>center</aligny>
        <align>center</align>
        <wrapmultiline>true</wrapmultiline>
      </control>
      
      <control type="image" >
            <description>page header tab icon</description>
            <posx>670</posx>
            <posy>12</posy>
            <width>200</width>
            <height>82</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[Container(120).ListItem.Icon]</texture>
      </control>
      <control type="image" >
            <description>page header tab icon</description>
            <posx>670</posx>
            <posy>12</posy>
            <width>200</width>
            <height>82</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[Container(120).ListItem.Thumb]</texture>
      </control>
      
      <control type="label">
        <description>textarea</description>
        <posx>30</posx>
        <posy>320</posy>
        <width>1050</width>
        <height>40</height>
        <font>font18</font>
        <textcolor>white</textcolor>
        <aligny>center</aligny>
        <align>center</align>
        
        <label>$INFO[Container(120).ListItem.Property(last)]</label>


      </control>
      
      <control type="image">
        <posx>20</posx>
        <posy>110</posy>
        <width>860</width>
        <height>2</height>
        <texture>TRseparator.png</texture>
      </control>
   
      <!-- ** Required ** Do not change <id> or <type> -->
      <control type="list" id="120">
        <posx>20</posx>
        <posy>126</posy>
        <width>820</width>
        <height>412</height>
        <onleft>120</onleft>
        <onright>120</onright>
        <onup>110</onup>
        <ondown>110</ondown>
        <viewtype label="535">list</viewtype>
        <pagecontrol>121</pagecontrol>
        <scrolltime>200</scrolltime>
        <itemlayout height="38">  
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>100</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>
          
          <control type="label">
            <posx>105</posx>
            <posy>0</posy>
            <width>545</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label2</info>
          </control>
          
          <control type="label">
            <posx>650</posx>
            <posy>0</posy>
            <width>140</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Property(date)</info>
          </control>
        </itemlayout>
        
        <focusedlayout height="38">
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>840</width>
            <height>38</height>
            <texture border="10">default-list-focus.png</texture>
            <visible>Control.HasFocus(120)</visible>
          </control>
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>100</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>
          
          <control type="label">
            <posx>105</posx>
            <posy>0</posy>
            <width>545</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label2</info>
          </control>
          
          <control type="label">
            <posx>650</posx>
            <posy>0</posy>
            <width>140</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Property(date)</info>
          </control>
        </focusedlayout>
      </control>
      <control type="group">
        <posx>855</posx>
        <posy>126</posy>
        <control type="scrollbar" id="121">
          <description>scroll bar indicator for lists</description>
          <posx>0</posx>
          <posy>0</posy>
          <width>12</width>
          <height>412</height>
          <texturesliderbackground border="6">default-scroll_back.png</texturesliderbackground>
          <texturesliderbar border="6">default-scroll_mid.png</texturesliderbar>
          <texturesliderbarfocus border="6">default-scroll_mid.png</texturesliderbarfocus>
          <textureslidernib>-</textureslidernib>
          <textureslidernibfocus>-</textureslidernibfocus>
          <orientation>vertical</orientation>
          <showonepage>false</showonepage>
          <visible>Control.IsVisible(120)</visible>
        </control>
      </control>

    </control>
  </controls>
</window>
