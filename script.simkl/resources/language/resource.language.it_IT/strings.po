msgctxt "#32001"
msgid "Please click to Log In"
msgstr "Clicca per accedere"

msgctxt "#32002"
msgid "Other options"
msgstr "Altre opzioni"

msgctxt "#32003"
msgid "Sync library"
msgstr "Libreria di sincronizzazione"

msgctxt "#32006"
msgid "Scrobble at percentage"
msgstr "Sincronizza dopo la visualizzazione"

msgctxt "#32007"
msgid "Checkin percentage"
msgstr "Percentuale di sincronizzazione"

msgctxt "#32008"
msgid "Show bubble when scrobbled"
msgstr "Mostra notifica durante la sincronizzazione"

msgctxt "#32009"
msgid "See options to customize Simkl TV Tracker"
msgstr "Vedi le opzioni per personalizzare Simkl TV Tracker"

msgctxt "#32010"
msgid "You are not logged in"
msgstr "Non sei loggato"

msgctxt "#32011"
msgid "You are logged in"
msgstr "Ti sei autenticato"

msgctxt "#32012"
msgid "Minimum file length for scrobbling (minutes)"
msgstr "Tempo di file minimo, in minuti, per abilitare la sincronizzazione"

msgctxt "#32021"
msgid "Simkl login"
msgstr "Accedi a Simkl"

msgctxt "#32022"
msgid "Please visit {0} on \n"
"computer or phone and enter this code"
msgstr "Prego, visita l'indirizzo {0} in\n"
"il tuo dispositivo e inserisci il seguente codice"

msgctxt "#32023"
msgid "Remaining time: {0}/{1}"
