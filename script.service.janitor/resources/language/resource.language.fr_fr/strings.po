# Frequency section
# =======================
msgctxt "#32200"
msgid "Frequency"
msgstr "Fréquence"

msgctxt "#32201"
msgid "[B]When should watched videos be cleaned?[/B]"
msgstr "[B]Quand la vidéo doit être nettoyée ?[/B]"

msgctxt "#32202"
msgid "Run as a service"
msgstr "Lancer en tant que service"

