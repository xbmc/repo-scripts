msgstr "Tu n'as pas internet"

msgctxt "#32028"
msgid "Scrobbling {0}"
msgstr ""

msgctxt "#32029"
msgid "Error at scrobbling. Retrying ({0})"
msgstr "Il y a eu une erreur de synchronisation. Réessayer ({0})"

msgctxt "#32030"
msgid "Login successful, {0}"
msgstr "Connexion réussie, {0}"

msgctxt "#32031"
msgid "Couldn't log in"
msgstr "Impossible de se connecter"

msgctxt "#32032"
msgid "You are already logged in as {0}"
msgstr "Vous êtes déjà connecté en tant que {0}"

msgctxt "#32033"
msgid "Continue"
