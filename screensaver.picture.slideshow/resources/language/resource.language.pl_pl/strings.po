msgstr "Przejścia"

msgctxt "#30009"
msgid "Pan and zoom"
msgstr "Efekt Kena Burnsa"

msgctxt "#30010"
msgid "Amount of seconds to display each image"
msgstr "Liczba sekund wyświetlania każdego obrazu"

msgctxt "#30011"
