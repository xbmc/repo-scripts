msgctxt "Addon Summary"
msgid "Download and Install Kodi packages."
msgstr "Lataa ja asenna Kodi-paketteja."

msgctxt "Addon Description"
msgid "Download and Install Kodi android packages; select from  Nightlies, Releases, Snapshots and Test-builds."
msgstr "Lataa ja asenna Kodin Android-paketteja kehityshaaroista Release, Nightly, Snapshot ja Test."

msgctxt "#30000"
msgid "Enable Debugging [Log errors]"
msgstr "Käytä vianselvitystä [kirjaa virheet]"

msgctxt "#30001"
msgid "Something went wrong, Try again later..."
msgstr "Jotain meni pieleen. Yritä myöhemmin uudelleen..."

msgctxt "#30002"
msgid "Preparing to download: %s"
msgstr "Valmistellaan latausta: %s"

msgctxt "#30003"
msgid "Keep all downloads?"
msgstr "Säilytetäänkö kaikki lataukset?"

msgctxt "#30004"
msgid "File already exists"
msgstr "Tiedosto on jo olemassa"

msgctxt "#30005"
msgid "Install"
msgstr "Asenna"

msgctxt "#30006"
msgid "Redownload"
msgstr "Lataa uudelleen"

msgctxt "#30007"
msgid "Installation complete, file removed"
msgstr "Asennus on valmis ja tiedosto poistettu"

msgctxt "#30008"
msgid "Cleanup download after installation"
msgstr "Siivoa lataus asennuksen jälkeen"

msgctxt "#30009"
msgid "Plugin disabled"
msgstr "Lisäosa ei ole käytössä"

msgctxt "#30010"
msgid "Unknown Android version"
