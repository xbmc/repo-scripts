msgid " were cleaned"
msgstr " zostało wyczyszczonych"

msgctxt "#32519"
msgid "Would you like to view the cleaning log?"
msgstr "Chcesz wyświetlić dziennik czyszczenia?"

msgctxt "#32520"
msgid "No cleaning required"
msgstr "Nie wymaga czyszczenia"

msgctxt "#32521"
msgid "The destination folder is not set up."
msgstr "Folder docelowy nie określony"

msgctxt "#32522"
msgid "You must first select a holding folder."
msgstr "Musisz najpierw podać folder zawierający"

msgctxt "#32523"
msgid "Would you like to do this now?"
msgstr "Czy chcesz to zrobić teraz?"


# Log section
# =======================

msgctxt "#32600"
msgid "History"
msgstr "Historia"

msgctxt "#32601"
msgid "[B]See which files have been cleaned recently.[/B]"
msgstr "[B]Zobacz jakie pliki ostatnio usunięto.[/B]"

msgctxt "#32602"
msgid "View cleaning history"
msgstr "Pokaż historię czyszczenia"

msgctxt "#32603"
msgid "[B]Cleaning Log[/B]"
msgstr "[B]Dziennik czyszczenia[/B]"

msgctxt "#32604"
msgid "Are you sure?"
msgstr "Jesteś pewny?"

msgctxt "#32605"
msgid "This will trim the log file, keeping the first 25 lines."
msgstr "Ta akcja ograniczy dziennik do pierwszych 25 linijek."

msgctxt "#32606"
msgid "This will delete the entire contents of the log file."
msgstr "Ta akcja wyczyści cały dziennik."

msgctxt "#32607"
msgid "This action is irreversible. Do you wish to continue?"
msgstr "Ta akcja jest nieodwracalna. Chcesz kontynuować?"

msgctxt "#32608"
msgid "Trim"
msgstr "Trymować"

msgctxt "#32609"
msgid "Clear"
msgstr "Oczyścić"
