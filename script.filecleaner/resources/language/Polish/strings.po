msgid "Error reading hard disk. Please check your settings."
msgstr "Błąd odczytu dysku twardego. Sprawdź swoje ustawienia."

msgctxt "#32512"
msgid "Could not check disk space. Access denied."
msgstr "Nie można sprawdzić wolnego miejsca. Brak dostępu."

msgctxt "#32513"
msgid "Could not check disk space. Path does not exist."
msgstr "Nie można sprawdzić wolnego miejsca. Ścieżka nie istnieje."

msgctxt "#32514"
msgid "Cleaning results"
msgstr "Wynik czyszczenia"

msgctxt "#32515"
msgid "movie(s)"
msgstr "film(-ów)"

msgctxt "#32516"
msgid "episode(s)"
msgstr "odcinek(-ów)"

msgctxt "#32517"
msgid "music video(s)"
msgstr "teledysk(-ów)"

#Suffix for cleaning results. Do not remove the single leading whitespace!
