<window>
	<animation effect="fade" time="400">WindowOpen</animation>
	<animation effect="fade" time="400">WindowClose</animation>
	<coordinates>
		<left>350</left>
		<top>135</top>
	</coordinates>
	<controls>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>1220</width>
			<height>712</height>
			<texture>rsseditor-panel.png</texture>
		</control>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>1220</width>
			<height>70</height>
			<texture colordiffuse="FF12B2E7" border="2">rsseditor-header.png</texture>
		</control>
		<control type="label" id="2">
			<textoffsetx>70</textoffsetx>
			<left>-30</left>
			<top>0</top>
			<width>1220</width>
			<height>70</height>
			<font>font20_title</font>
			<align>left</align>
			<aligny>center</aligny>
			<shadowcolor>black</shadowcolor>
		</control>
		<control type="label" id="4">
			<description>List Label</description>
			<textoffsetx>70</textoffsetx>
			<left>-30</left>
			<top>70</top>
			<width>1220</width>
			<height>70</height>
			<font>font20</font>
			<align>left</align>
			<aligny>center</aligny>
		</control>
		<control type="image">
			<left>0</left>
			<top>119</top>
			<width>920</width>
			<height>596</height>
			<texture border="40">rsseditor-button-nofocus.png</texture>
		</control>
		<control type="list" id="10">
			<left>20</left>
			<top>139</top>
			<width>880</width>
			<height>556</height>
			<onup>10</onup>
			<ondown>10</ondown>
			<onleft>9001</onleft>
			<onright>9001</onright>
			<scrolltime>200</scrolltime>
			<itemlayout height="69">
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>880</width>
					<height>69</height>
					<texture border="40">lists/separator.png</texture>
				</control>
				<control type="label">
					<left>0</left>
					<textoffsetx>20</textoffsetx>
					<top>0</top>
					<width>880</width>
					<height>69</height>
					<align>$PARAM[align]</align>
					<aligny>center</aligny>
					<textcolor>grey</textcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout height="69">
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>880</width>
					<height>69</height>
					<texture border="40">lists/separator.png</texture>
				</control>
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>880</width>
					<height>69</height>
					<texture colordiffuse="FF12B2E7">lists/focus.png</texture>
					<animation effect="fade" start="100" end="40" time="100" condition="ControlGroup(9001).HasFocus">Conditional</animation>
				</control>
				<control type="label">
					<left>0</left>
					<textoffsetx>20</textoffsetx>
					<top>0</top>
					<width>880</width>
					<height>69</height>
					<align>$PARAM[align]</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</focusedlayout>
		</control>
		<control type="label">
			<description>number of files/pages in list text label</description>
			<left>925</left>
			<top>626</top>
			<width>275</width>
			<height>35</height>
			<font>font12</font>
			<align>right</align>
			<aligny>center</aligny>
			<textcolor>grey</textcolor>
			<label>[COLOR=FF12B2E7]$INFO[Container(10).NumItems][/COLOR] $LOCALIZE[31036] - $LOCALIZE[31035] [COLOR=FF12B2E7]$INFO[Container(10).CurrentPage]/$INFO[Container(10).NumPages][/COLOR]</label>
		</control>
		<control type="grouplist" id="9001">
			<left>920</left>
			<top>80</top>
			<onup>9001</onup>
			<ondown>9001</ondown>
			<onleft>10</onleft>
			<onright>10</onright>
			<itemgap>-20</itemgap>
			<control type="button" id="13">
				<width>300</width>
				<height>90</height>
				<label>15019</label>
				<font>font12_title</font>
