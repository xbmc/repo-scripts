msgctxt "#32001"
msgid "Please click to Log In"
msgstr "S'il vous plait, cliquez pour vous connecter"

msgctxt "#32002"
msgid "Other options"
msgstr "Autres options"

msgctxt "#32003"
msgid "Sync library"
msgstr "Synchroniser la bibliothèque"

msgctxt "#32006"
msgid "Scrobble at percentage"
msgstr "Synchroniser après avoir visionné"

msgctxt "#32007"
msgid "Checkin percentage"
msgstr "Pourcentage de synchronisation"

msgctxt "#32008"
msgid "Show bubble when scrobbled"
msgstr "Afficher la notification lors de la synchronisation"

msgctxt "#32009"
msgid "See options to customize Simkl TV Tracker"
msgstr "Voir les options pour personnaliser Simkl TV Tracker"

msgctxt "#32010"
msgid "You are not logged in"
msgstr "Vous n'êtes pas connecté"

msgctxt "#32011"
msgid "You are logged in"
msgstr "Vous êtes authentifié"

msgctxt "#32012"
msgid "Minimum file length for scrobbling (minutes)"
msgstr "Durée minimale du fichier, en minutes, pour activer la synchronisation"

msgctxt "#32021"
msgid "Simkl login"
msgstr "Connectez-vous à Simkl"

msgctxt "#32022"
msgid "Please visit {0} on \n"
"computer or phone and enter this code"
msgstr "Veuillez visiter à l'adresse {0} sur\n"
"votre appareil et entrez le code suivant"

msgctxt "#32023"
msgid "Remaining time: {0}/{1}"
