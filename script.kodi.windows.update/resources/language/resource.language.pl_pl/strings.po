msgctxt "Addon Summary"
msgid "Download and Install Kodi packages."
msgstr "Pobieraj i instaluj pakiety Kodi."

msgctxt "Addon Description"
msgid "Download and Install Kodi window executables; select from  Nightlies, Releases, Snapshots and Test-builds."
msgstr "Pobieraj i instaluj pliki wykonywalne Kodi: kompilacje nocne, testowe, wydania finalne, migawki."

msgctxt "Addon Disclaimer"
msgid "UWP not supported! Not intended for Kodi installation through the Windows Store."
msgstr "Platforma UWP nie jest obsługiwana! Nie jest przeznaczony do instalacji Kodi za pośrednictwem Windows Store."

msgctxt "#30000"
msgid "Enable Debugging [Log errors]"
msgstr "Włącz debugowanie [rejestrowanie błędów]"

msgctxt "#30001"
msgid "Something went wrong, Try again later..."
msgstr "Coś poszło nie tak, spróbuj ponownie później..."

msgctxt "#30002"
msgid "Downloading..."
msgstr "Pobieranie..."

msgctxt "#30003"
msgid "Keep all downloads?"
msgstr "Zachować wszystkie pobrane pliki?"

msgctxt "#30004"
msgid "File already exists"
msgstr "Plik już istnieje"

msgctxt "#30005"
msgid "Install"
msgstr "Zainstaluj"

msgctxt "#30006"
msgid "Redownload"
msgstr "Pobierz ponownie"

msgctxt "#30007"
msgid "Installation Complete, File Removed"
msgstr "Instalacja zakończona, plik usunięty"

msgctxt "#30008"
msgid "Cleanup download after installation"
msgstr "Czyszczenie pobierania po instalacji"

msgctxt "#30009"
msgid "UWP Installations Not Supported!"
msgstr "Instalacje platformy UWP nie są obsługiwane!"

msgctxt "#30010"
msgid "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"
msgstr "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"

msgctxt "#30011"
msgid "Plugin disabled"
msgstr "Wtyczka wyłączona"

msgctxt "#30012"
msgid "Would you like to disable this plugin?"
msgstr "Czy chcesz wyłączyć tę wtyczkę?"

msgctxt "#30014"
msgid "Current Windows Platform (%s)"
msgstr "Bieżąca platforma Windows (%s)"

msgctxt "#30015"
msgid "Semi-stable (Monthly builds)."
msgstr "Półstabilna (kompilacje miesięczne)."

msgctxt "#30016"
msgid "Stable (Final releases)."
msgstr "Stabilna (ostateczne wydania)."

msgctxt "#30017"
msgid "Unstable (Daily builds)."
msgstr "Niestabilna (kompilacje dzienne)."

msgctxt "#30018"
msgid "Experimental (Debugging builds)."
msgstr "Eksperymentalna (kompilacje debugowania)."

msgctxt "#30019"
msgid "Incompatible platform"
msgstr "Niezgodna platforma"

msgctxt "#30020"
msgid "Enable Silent Installation"
msgstr "Włącz cichą instalację"

msgctxt "#30021"
msgid "Current Kodi Branch (%s.%s-%s)"
msgstr "Bieżąca gałąź Kodi (%s.%s-%s)"

