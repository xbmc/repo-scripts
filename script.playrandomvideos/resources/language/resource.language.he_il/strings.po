# Commands
msgctxt "#32100"
msgid "Play Random"
msgstr "נגן אקראי"

msgctxt "#32101"
msgid "Disable 'Play Random' context item [I](per skin)[/I]"
msgstr "השבת את פריט ניגון אקראי מתפריט הקישור[I](לכל מעטפת)[/I]"

msgctxt "#32102"
msgid "Enable 'Play Random' context item [I](per skin)[/I]"
msgstr "אפשר את פריט ניגון אקראי מתפריט הקישור[I](לכל מעטפת)[/I]"

# Headings
msgctxt "#32010"
msgid "Filter random selection by watched status"
msgstr "סנן בחירה אקראית לפי סטטוס נצפה"

msgctxt "#32011"
msgid "Can not play from non-permanent video source"
msgstr "לא ניתן לנגן ממקור וידאו לא קבוע"

# {0} = path to selected directory
msgctxt "#32012"
msgid "Add this or a higher directory as a permanent video source and try again.[CR]{0}"
msgstr "הוסף את השם מטה או בחר ספריית אב (גבוהה יותר) כמקור וידאו קבוע ונסה שוב.[CR]{0}"

# Settings
msgctxt "#32905"
msgid "Hide busy notification before first video"
msgstr "הסתר את הודעת עסוק לפני הסרטון הראשון"

#  heading
msgctxt "#32900"
msgid "Watched Filters"
msgstr "מסנני סטטוס נצפה"

msgctxt "#32901"
msgid "Filter movies by:"
msgstr "סנן סרטים לפי:"

msgctxt "#32902"
msgid "Filter TV shows by:"
msgstr "סנן סדרות לפי:"

msgctxt "#32903"
msgid "Filter music videos by:"
msgstr "סנן סרטוני מוזיקה לפי:"

msgctxt "#32904"
msgid "Filter other videos by:"
msgstr "סנן קבצי וידאו אחרים לפי:"
