msgctxt "#32001"
msgid "Please click to Log In"
msgstr "Por favor, clica para Iniciar Sessão"

msgctxt "#32002"
msgid "Other options"
msgstr "Outras opções"

msgctxt "#32003"
msgid "Sync library"
msgstr "Sincronizar biblioteca"

msgctxt "#32006"
msgid "Scrobble at percentage"
msgstr "Sincronizar após visualizar"

msgctxt "#32007"
msgid "Checkin percentage"
msgstr "Percentagem de sincronização"

msgctxt "#32008"
msgid "Show bubble when scrobbled"
msgstr "Mostrar notificação quando sincronizar"

msgctxt "#32009"
msgid "See options to customize Simkl TV Tracker"
msgstr "Veja as opções para personalizar Simkl TV Tracker"

msgctxt "#32010"
msgid "You are not logged in"
msgstr "Sessão Não Iniciada"

msgctxt "#32011"
msgid "You are logged in"
msgstr "Sessão Iniciada"

msgctxt "#32012"
msgid "Minimum file length for scrobbling (minutes)"
msgstr "Tempo mínimo do ficheiro, em minutos, para ativar a sincronização"

msgctxt "#32021"
msgid "Simkl login"
msgstr "Iniciar Sessão em Simkl"

msgctxt "#32022"
msgid "Please visit {0} on \n"
"computer or phone and enter this code"
msgstr "Por favor, visita o endereço {0} no\n"
"seu dispositivo e digite o seguinte código"

msgctxt "#32023"
msgid "Remaining time: {0}/{1}"
msgstr "Tempo restante: {0}/{1}"

msgctxt "#32025"
