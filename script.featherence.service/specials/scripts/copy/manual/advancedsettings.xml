<!-- HTPT -->
<advancedsettings>
	<splash>false</splash>
	<loglevel>0</loglevel>
	<navigatevirtualkeyboard>true</navigatevirtualkeyboard>
    <video>
        <ignorepercentatend>3</ignorepercentatend>
        <ignoresecondsatstart>300</ignoresecondsatstart>
        <playcountminimumpercent>90</playcountminimumpercent>
        <subsdelayrange>200</subsdelayrange>
    </video>
    <audio>
        <resample>48000</resample>
        <streamsilence>1</streamsilence>
    </audio>
	<lookandfeel>
		<enablerssfeeds>false</enablerssfeeds>
    </lookandfeel>
    <network>
        <buffermode>0</buffermode>
        <cachemembuffersize>0</cachemembuffersize>
        <curlclienttimeout>10</curlclienttimeout>
        <curllowspeedtime>5</curllowspeedtime>
		<disableipv6>true</disableipv6>
        <readbufferfactor>4.0</readbufferfactor>
    </network>
    <pvr>
        <cacheindvdplayer>true</cacheindvdplayer>
        <maxvideocachelevel>20</maxvideocachelevel>
        <minvideocachelevel>10</minvideocachelevel>
    </pvr>
    <samba>
        <clienttimeout>100</clienttimeout>
        <statfiles>false</statfiles>
    </samba>
    <enableairtunesdebuglog>false</enableairtunesdebuglog>
    <fanartres>1080</fanartres>
    <gui>
        <algorithmdirtyregions>3</algorithmdirtyregions>
    </gui>
    <gui>
        <nofliptimeout>0</nofliptimeout>
    </gui>
    <imageres>1080</imageres>
    <packagefoldersize>250</packagefoldersize>
    <playlistretries>10</playlistretries>
    <playlisttimeout>3</playlisttimeout>
    <showexitbutton>false</showexitbutton>
    <skiploopfilter>8</skiploopfilter>
    <useddsfanart>false</useddsfanart>
    <videoscanner>
        <ignoreerrors>true</ignoreerrors>
    </videoscanner>
</advancedsettings>