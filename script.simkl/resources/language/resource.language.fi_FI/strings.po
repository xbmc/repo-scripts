msgctxt "#32001"
msgid "Please click to Log In"
msgstr "Klikkaa kirjautuaksesi sisään"

msgctxt "#32002"
msgid "Other options"
msgstr "Muita asetuksia"

msgctxt "#32003"
msgid "Sync library"
msgstr "Synkronoi kirjasto"

msgctxt "#32006"
msgid "Scrobble at percentage"
msgstr "Prosenttiarvo, jossa toistotieto tallennetaan"

msgctxt "#32007"
msgid "Checkin percentage"
msgstr "Tarkistetaan prosentteja"

msgctxt "#32008"
msgid "Show bubble when scrobbled"
msgstr "Näytä kupla kun toistotieto on tallennettu"

msgctxt "#32009"
msgid "See options to customize Simkl TV Tracker"
msgstr "Näytä ilmoitus kun toistotieto on tallennettu"

msgctxt "#32010"
msgid "You are not logged in"
msgstr "Et ole kirjautunut"

msgctxt "#32011"
msgid "You are logged in"
msgstr "Olet sisäänkirjautunut"

msgctxt "#32012"
msgid "Minimum file length for scrobbling (minutes)"
msgstr "Tiedoston vähimmäiskesto seurantaa varten (minutteina)"

msgctxt "#32021"
msgid "Simkl login"
msgstr "Simkl kirjatuminen"

msgctxt "#32022"
msgid "Please visit {0} on \n"
"computer or phone and enter this code"
msgstr "Vieraile osoitteessa {0} teitokoneellasi tai mobiililaitteellasi ja syötä tämä koodi"

msgctxt "#32023"
msgid "Remaining time: {0}/{1}"
msgstr "Jäljellä oleva aika: {0}/{1}"

msgctxt "#32025"
msgid "Hello again, {0}"
msgstr "Hei taas, {0}"

msgctxt "#32026"
msgid "Please Log In in addon settings"
msgstr "Ole hyvä ja kirjaudu palveluun laajennuksen asetusten kautta"

msgctxt "#32027"
msgid "You don't have internet"
msgstr "Sinulla ei ole internetyhteyttä"

msgctxt "#32028"
msgid "Scrobbling {0}"
msgstr ""

msgctxt "#32029"
msgid "Error at scrobbling. Retrying ({0})"
msgstr "Virhe toistotilan tallennuksessa. Yritetään uudestaan ({0})"

msgctxt "#32030"
msgid "Login successful, {0}"
msgstr "Kirjautuminen onnistui, {0}"

msgctxt "#32031"
msgid "Couldn't log in"
msgstr "Kirjautuminen ei onnistunut"

msgctxt "#32032"
msgid "You are already logged in as {0}"
msgstr "Olet kirjautunut käyttäjänä {0}"

msgctxt "#32033"
msgid "Continue"
msgstr "Jatka"

msgctxt "#32034"
msgid "Cancel"
msgstr "Peruuta"

msgctxt "#32080"
msgid "Failed to scrobble, retrying later"
msgstr "Toistotilan tallennus epäonnistui, yritetää myöhemmin uudelleen"

msgctxt "#32081"
msgid "Finally, failed to scrobble"
msgstr "Loppujen lopuksi, toistotilan tallennus epäonnistui"

