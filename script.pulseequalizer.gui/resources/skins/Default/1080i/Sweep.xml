<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">3000</defaultcontrol>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	<coordinates>
		<left>510</left>
		<top>360</top>
		<origin x="510" y="360">Window.IsActive(Home)</origin>
	</coordinates>
	<controls>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>800</width>
			<height>260</height>
			<texture colordiffuse="{col_background}">colors/white.png</texture>
		</control>
		<control type="progress" id="1900">
			<description>Progress</description>
			<left>50</left>
			<top>100</top>
			<width>700</width>
			<height>20</height>
			<reveal>true</reveal>
			<texturebg>progress/bk.png</texturebg>
			<midtexture>progress/mid.png</midtexture>

		</control>
		<control type="progress" id="1901">
			<description>Progress</description>
			<left>50</left>
			<top>150</top>
			<width>700</width>
			<height>20</height>
			<reveal>true</reveal>
			<texturebg>progress/bk.png</texturebg>
			<midtexture>progress/mid.png</midtexture>
			
		</control>
		<control type="button" id="3000">
			<textoffsetx>50</textoffsetx>
			<left>0</left>
			<top>280</top>
			<width>800</width>
			<height>65</height>
			<label>Stop</label>
			<textcolor>{col_text}</textcolor>
			<selectedcolor>{col_select}</selectedcolor>
			<focusedcolor>{col_textfocus}</focusedcolor>
			<font>{font}</font>
			<texturefocus colordiffuse="{col_texturefocus}">colors/white.png</texturefocus>
			<texturenofocus colordiffuse="{col_texturenofocus}">colors/white.png</texturenofocus>
			<align>center</align>
			<aligny>center</aligny>
			<onup>3003</onup>
		</control>

	</controls>
</window>
