msgctxt "#32027"
msgid "You don't have internet"
msgstr "Non hai internet"

msgctxt "#32028"
msgid "Scrobbling {0}"
msgstr ""

msgctxt "#32029"
msgid "Error at scrobbling. Retrying ({0})"
msgstr "Si è verificato un errore di sincronizzazione. Riprova ({0})"

msgctxt "#32030"
msgid "Login successful, {0}"
msgstr "Connessione riuscita, {0}"

msgctxt "#32031"
msgid "Couldn't log in"
msgstr "Impossibile connettersi"

msgctxt "#32032"
msgid "You are already logged in as {0}"
msgstr "Hai già effettuato l'accesso come {0}"

msgctxt "#32033"
msgid "Continue"
msgstr "Continuare"

msgctxt "#32034"
msgid "Cancel"
msgstr "Annullare"

msgctxt "#32080"
msgid "Failed to scrobble, retrying later"
msgstr "Impossibile sincronizzare i servizi, riproveremo presto"

msgctxt "#32081"
