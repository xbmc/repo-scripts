msgstr "Redescargar"

msgctxt "#30007"
msgid "Installation Complete, File Removed"
msgstr "Instalación completa, archivo eliminado"

msgctxt "#30008"
msgid "Cleanup download after installation"
msgstr "Limpiar descarga después de instalación"

msgctxt "#30009"
msgid "UWP Installations Not Supported!"
msgstr "¡Instalaciones UWP no soportadas!"

msgctxt "#30010"
