msgctxt "#30014"
msgid "Current Android platform (%s)"
msgstr "Nykyinen Android-alusta (%s)"

msgctxt "#30015"
msgid "Semi-stable (Monthly builds)."
msgstr "Puolivakaa (kuukausittaiset versiot)."

msgctxt "#30016"
msgid "Stable (Final releases)."
msgstr "Vakaa (julkaisuversiot)."

msgctxt "#30017"
