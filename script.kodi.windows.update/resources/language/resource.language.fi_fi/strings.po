msgctxt "Addon Summary"
msgid "Download and Install Kodi packages."
msgstr "Lataa ja asenna Kodi-paketteja."

msgctxt "Addon Description"
msgid "Download and Install Kodi window executables; select from  Nightlies, Releases, Snapshots and Test-builds."
msgstr "Lataa ja asenna Kodin Windows-asennuksia kehityshaaroista Release, Nightly, Snapshot ja Test."

msgctxt "Addon Disclaimer"
msgid "UWP not supported! Not intended for Kodi installation through the Windows Store."
msgstr "UWP-versiota ei tueta! Lisäosaa ei ole tarkoitettu Microsoft Storesta asennetuille Kodi-asennuksille."

msgctxt "#30000"
msgid "Enable Debugging [Log errors]"
msgstr "Käytä vianselvitystä [kirjaa virheet]"

msgctxt "#30001"
msgid "Something went wrong, Try again later..."
msgstr "Jotain meni pieleen. Yritä myöhemmin uudelleen..."

msgctxt "#30002"
msgid "Downloading..."
msgstr "Ladataan..."

msgctxt "#30003"
msgid "Keep all downloads?"
msgstr "Säilytetäänkö kaikki lataukset?"

msgctxt "#30004"
msgid "File already exists"
msgstr "Tiedosto on jo olemassa"

msgctxt "#30005"
msgid "Install"
msgstr "Asenna"

msgctxt "#30006"
msgid "Redownload"
msgstr "Lataa uudelleen"

msgctxt "#30007"
msgid "Installation Complete, File Removed"
msgstr "Asennus on valmis ja tiedosto poistettu"

msgctxt "#30008"
msgid "Cleanup download after installation"
msgstr "Siivoa lataus asennuksen jälkeen"

msgctxt "#30009"
msgid "UWP Installations Not Supported!"
msgstr "UWP-asennuksia ei tueta!"

msgctxt "#30010"
msgid "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"
msgstr "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"

msgctxt "#30011"
msgid "Plugin disabled"
msgstr "Lisäosa ei ole käytössä"

msgctxt "#30012"
msgid "Would you like to disable this plugin?"
msgstr "Haluatko poistaa lisäosan käytöstä?"

msgctxt "#30014"
msgid "Current Windows Platform (%s)"
msgstr "Nykyinen Windows-alusta (%s)"

msgctxt "#30015"
msgid "Semi-stable (Monthly builds)."
msgstr "Puolivakaa (kuukausittaiset versiot)."

msgctxt "#30016"
msgid "Stable (Final releases)."
msgstr "Vakaa (julkaisuversiot)."

msgctxt "#30017"
msgid "Unstable (Daily builds)."
msgstr "Epävakaa (päivittäiset versiot)."

msgctxt "#30018"
msgid "Experimental (Debugging builds)."
msgstr "Kokeellinen (vianselvitysversiot)."

msgctxt "#30019"
msgid "Incompatible platform"
msgstr "Yhteensopimaton alusta"

msgctxt "#30020"
msgid "Enable Silent Installation"
msgstr "Käytä huomaamatonta asennusta"

msgctxt "#30021"
msgid "Current Kodi Branch (%s.%s-%s)"
msgstr "Nykyinen Kodi-haara (%s.%s-%s)"

