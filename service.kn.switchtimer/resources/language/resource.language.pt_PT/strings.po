msgctxt "Addon Summary"
msgid "A switch timer for the PVR module of Kodi"
msgstr ""

msgctxt "Addon Description"
msgid "Switching with the KN switchtimer from every point of view inside Kodi to a tv or radio broadcast at a given time - no more miss your favorite shows or sports events on tv."
msgstr ""

msgctxt "Addon Disclaimer"
msgid "Use at your own risk."
msgstr ""

msgctxt "#30000"
msgid "Switch Timer Service"
msgstr "Serviço de Sintonização Agendada"

msgctxt "#30001"
msgid "Show Notice or Countdown before Channel will switched"
msgstr "Mostrar Notificação ou Contagem decrescente antes de sintonizar o canal"

msgctxt "#30002"
msgid "Duration of Notification or Countdown"
msgstr "Duração da Notificação ou Contagem decrescente"

msgctxt "#30003"
msgid "Show Confirmation if Timer is set"
msgstr "Mostrar confirmação se o temporizador estiver definido"

msgctxt "#30004"
msgid "Ignore Timers older than"
msgstr "Ignorar temporizadores com mais de"

msgctxt "#30010"
msgid "Currently active Timers"
msgstr "Temporizadores atualmente ativos"

msgctxt "#30011"
msgid "Delete all Timers"
msgstr "Eliminar todos os temporizadores"

msgctxt "#30012"
msgid "Use Countdown Timer"
msgstr "Usar temporizador com contagem decrescente"

msgctxt "#30020"
msgid "Date/Time of Timer could not calculated"
msgstr "Não foi possível calcular a data/hora do temporizador"

msgctxt "#30021"
msgid "Timer added"
msgstr "Temporizador adicionado"

msgctxt "#30022"
msgid "Timer is in the past"
msgstr "A data/hora de ínicio já passou"

msgctxt "#30023"
msgid "Timer for this time already set"
msgstr "Já existe um temporizador para esta hora"

msgctxt "#30024"
msgid "Timer Limit exceed, no free Slots"
msgstr "Limite de temporizadores atingido, não existem vagas"

msgctxt "#30025"
msgid "Could not switch to %s"
msgstr "Não foi possível sintonizar %s"

msgctxt "#30026"
msgid "Switching to '%s/%s' in a few seconds"
msgstr "'%s/%s' vai começar em poucos segundos"

msgctxt "#30027"
msgid "Broadcast on '%s' starts now"
msgstr "A emissão '%s' começa agora"

msgctxt "#30028"
msgid "Channel Switch requested"
msgstr "Sintonização agendada de canal"

msgctxt "#30029"
msgid "%s Seconds left to abort"
msgstr "%s segundos restantes"

msgctxt "#30030"
msgid "Please use the PVR OSD Guide context menue to set or reset switch timers"
msgstr ""

msgctxt "#30031"
msgid "Timer for this time already set (%s, %s). Would You replace this timer with the new one instead?"
msgstr ""

msgctxt "#30032"
msgid "Channel Switch aborted"
msgstr ""

msgctxt "#30040"
msgid "Add switchtimer"
msgstr "Novo temporizador de sintonização"

msgctxt "#30041"
msgid "Reset all switchtimers"
msgstr "Reiniciar temporizadores de sintonização"
