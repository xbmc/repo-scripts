msgctxt "Addon Summary"
msgid "Download and Install Kodi packages."
msgstr "Preuzmi i instaliraj Kodi pakete."

msgctxt "Addon Description"
msgid "Download and Install Kodi window executables; select from  Nightlies, Releases, Snapshots and Test-builds."
msgstr "Preuzmi i instaliraj Kodi window izvršne datoteke; odaberi iz  Nightlies, Releases, Snapshots i Testnih-izgradnja."

msgctxt "Addon Disclaimer"
msgid "UWP not supported! Not intended for Kodi installation through the Windows Store."
msgstr "UWP nije podržan! Nije namijenjeno za Kodi instalaciju putem Windows trgovine."

msgctxt "#30000"
msgid "Enable Debugging [Log errors]"
msgstr "Omogući otklanjanje grešaka [Zapisi grešaka]"

msgctxt "#30001"
msgid "Something went wrong, Try again later..."
msgstr "Nešto je pošlo po krivu, pokušajte kasnije ponovno..."

msgctxt "#30002"
msgid "Downloading..."
msgstr "Preuzimanje..."

msgctxt "#30003"
msgid "Keep all downloads?"
msgstr "Zadrži sva preuzimanja?"

msgctxt "#30004"
msgid "File already exists"
msgstr "Datoteka već postoji"

msgctxt "#30005"
msgid "Install"
msgstr "Instaliraj"

msgctxt "#30006"
msgid "Redownload"
msgstr "Ponovno preuzmi"

msgctxt "#30007"
msgid "Installation Complete, File Removed"
msgstr "Instalacija je završena, datoteka je uklonjena"

msgctxt "#30008"
msgid "Cleanup download after installation"
msgstr "Očisti preuzimanja nakon instalacije"

msgctxt "#30009"
msgid "UWP Installations Not Supported!"
msgstr "UWP instalacija nije podržana!"

msgctxt "#30010"
msgid "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"
msgstr "/../../Users/*/AppData/Local/Packages/XBMCFoundation.Kodi_*"

