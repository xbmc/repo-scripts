msgctxt "#32027"
msgid "You don't have internet"
msgstr "Não estás conetado à Internet"

msgctxt "#32028"
msgid "Scrobbling {0}"
msgstr "Scrobbling {0}"

msgctxt "#32029"
msgid "Error at scrobbling. Retrying ({0})"
msgstr "Ocorreu um erro na sincronização. Tentando novamente ({0})"

msgctxt "#32030"
msgid "Login successful, {0}"
msgstr "Sessão Iniciada, {0}"

msgctxt "#32031"
msgid "Couldn't log in"
msgstr "Não foi possível Iniciar Sessão"

msgctxt "#32032"
msgid "You are already logged in as {0}"
msgstr "Iniciaste Sessão como '{0}'"

msgctxt "#32033"
msgid "Continue"
msgstr "Continuar"

msgctxt "#32034"
msgid "Cancel"
msgstr "Cancelar"

msgctxt "#32080"
msgid "Failed to scrobble, retrying later"
msgstr "Não foi possível sincronizar os serviços, tentaremos de novo brevemente"

msgctxt "#32081"
msgid "Finally, failed to scrobble"
msgstr "Infelizmente, não foi possível sincronizar"

